<% layout('boilerplate') %>
<body id="allSubjects-page">
    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 row-cols-3">
        <% for (let subject of allSubjects) { %>
          <div class="card col" onclick="window.location.href='/'">
            <img src="" class="card-img-top" alt="listing_image" style="height: 20rem;">
            <div class="card-img-overlay"></div>
            <div class="card-body">
              <p class="card-text">
                <b><%= subject %></b><br>
              </p>
            </div>
          </div>
        <% } %>
</body>